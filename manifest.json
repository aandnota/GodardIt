{
	"manifest_version": 2,

	"name": "Godard It",
	"description": "French new wave-ify a page",
	"version": "1.0",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" },

	"permissions": [
		"tabs", "*://*/*"
	],
	"browser_action": {
		"default_icon": {
			"19": "icon19.png",
			"38": "icon38.png"
		},
		"default_title": "Godard It"
	},
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	}
}